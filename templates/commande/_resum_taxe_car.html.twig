<table class="table table-bordered table-striped mx-auto">
    <tr>
        <th>Taxe Regionale</th>
        <td>{{ taxe.TaxeRegionale | displayValue }}
            €</td>
    </tr>
    <tr>
        <th>Taxe > 35CV</th>
        <td>{{ taxe.Taxe35cv | displayValue }}
            €</td>
    </tr>
    <tr>
        <th>Taxe CO2</th>
        <td>{{ taxe.TaxeCO2 | displayValue}}
            €</td>
    </tr>
    <tr>
        <th>Taxe Malus</th>
        <td>{{ taxe.TaxeMalus | displayValue}}
            €</td>
    </tr>
    <tr>
        <th>Taxe ParaFiscale</th>
        <td>{{ taxe.TaxeParafiscale | displayValue}}
            €</td>
    </tr>
    <tr>
        <th>Taxe SIV</th>
        <td>{{ taxe.TaxeSIV | displayValue}}
            €</td>
    </tr>
    <tr>
        <th>Taxe d'acheminement</th>
        <td>{{ taxe.TaxeRedevanceSIV | displayValue}} €</td>
    </tr>
    <tr>
        <th>Frais de traitement de dossier</th>
        <td>
            {{ fraisTotalTraitement(commande) }} €
        </td>
    </tr>
    <tr>
        <th>TOTAL:</th>
        <td>
            <strong>{{ fraisTotal(commande) }} €</strong>
        </td>
    </tr>
</table>