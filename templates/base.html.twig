	<script type="text/javascript">
		$(function () {
			$('textarea.ckeditor').ckeditor();
		})
		$('#myModal').on('shown.bs.modal', function () {
			$('#myInput').trigger('focus')
		});  
		$( function() {
			$( ".js-datepicker" ).datepicker();
		} );
	</script>
{% endblock %}
</body>
</html>
